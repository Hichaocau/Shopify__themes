{% schema %}
{
    "name": "Footer Vegetable",
    "class": "main",
    "settings": [
        
    ],
    "blocks": [
        {
          "type": "info",
          "name": "Information",
          "settings":[
                {
                    "type": "text",
                    "id": "heading",
                    "label": "Name shop",
                    "default": "Name shop"
                },
                {
                    "type": "image_picker",
                    "label": "Image logo",
                    "id": "logo"
                },
                {
                    "type": "text",
                    "id": "description",
                    "label": "Description",
                    "default": "We ara a lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat..."
                },
                {
                    "type": "text",
                    "id": "more",
                    "label": "Read more",
                    "default": "Read more"
                },
                {
                    "type": "url",
                    "id": "more_link",
                    "label": "Link"
                },
                {
                    "type": "text",
                    "id": "phone",
                    "label": "Phone",
                    "default": "305-246-9248"
                },
                {
                    "type": "text",
                    "id": "email",
                    "label": "Email",
                    "default": "czarshop@gmail.com"
                },
                {
                    "type": "text",
                    "id": "location",
                    "label": " Location",
                    "default": "301 Campfire Meriden Uk"
                }
            ]  
        },
        {
            "type": "Column",
            "name": "Column",
            "limit": 3,
            "settings":[
                {
                    "type": "text",
                    "id": "Column_heading",
                    "label": "Heading",
                    "default": "CATEGORIES"
                },
                {
                    "type": "link_list",
                    "id": "menu",
                    "default": "footer",
                    "label": "Menu"
                }
            ]
        }
    ],
    "presets": [
        {
            "name": "Footer Vegetable",
            "blocks": [
                {
                    "type": "info"
                },
                {
                    "type": "Column"
                },
                {
                    "type": "Column"
                },
                {
                    "type": "Column"
                }
            ]
        }
    ]
}
{% endschema %}